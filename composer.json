{
  "name": "michielkempen/nova-order-field",
  "description": "A Laravel Nova field.",
  "keywords": [
    "laravel",
    "nova"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "laravel/nova": "^4.0",
    "spatie/eloquent-sortable": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "MichielKempen\\NovaOrderField\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MichielKempen\\NovaOrderField\\OrderFieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
